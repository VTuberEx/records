{
	"$schema": "http://json.schemastore.org/tsconfig",
	"extends": "@app/local-rig/profiles/default/src/tsconfig.json",
	"compilerOptions": {
		"module": "ESNext",
		"outDir": "../temp/tsc",
		"rootDir": "./",
		"lib": ["DOM", "ESNext"],
		"sourceRoot": "app://debug/",
		"resolveJsonModule": true,
		"jsx": "react-jsx",
		"jsxImportSource": "react",
		"rootDirs": ["./", "../temp/sass-ts/"],
		"baseUrl": "./",
		"noEmit": true,
		"typeRoots": [/*"../../../scripts/types", */ "../node_modules/@types/", "../node_modules"],
		"types": [],
		"strictNullChecks": false,
		"strictPropertyInitialization": false
	}
}
